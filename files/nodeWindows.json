{
  "capabilities":
  [
    {
      "browserName": "firefox",
      "marionette": true,
      "maxInstances": 10,
      "seleniumProtocol": "WebDriver"
    },
    {
      "browserName": "chrome",
      "maxInstances": 10,
      "seleniumProtocol": "WebDriver"
    },
    {
      "browserName": "opera",
      "platform": "WIN10",
      "maxInstances": 10,
      "seleniumProtocol": "WebDriver",
	  "webdriver.chrome.driver": "C:/bin/operadriver.exe"
    },
    {
      "browserName": "yandex",
      "platform": "WIN10",
      "maxInstances": 5,
	  "seleniumProtocol": "WebDriver",
	  "webdriver.chrome.driver": "C:/bin/yandexdriver.exe"
    },
	{
      "browserName": "MicrosoftEdge",
      "platform": "WIN10",
      "maxInstances": 5,
      "seleniumProtocol": "WebDriver",
	  "webdriver.edge.driver": "C:/bin/msedgedriver.exe"
    }
